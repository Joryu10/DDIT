<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 
	[마이바티스] 스프링에서 "_"를 사용한 컬럼명을 사용 시(BOOK 테이블의 BOOK_ID)
	카멜케이스로 읽어줌(bookId), INSERT_DATE => insertDate
	ex) 테이블 컬러명이 member_id인 경우 jsp화면단에서 이 값을 사용 시 memberId로 사용
	-->
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	<!-- 자주 사용하는 타입의 별칭을 세팅 -->
	<typeAliases>
		<typeAlias type="kr.or.ddit.vo.ComCodeInfoVO" alias="comCodeInfoVO" />
		<typeAlias type="kr.or.ddit.vo.ComDetCodeInfoVO" alias="comDetCodeInfoVO" />
		<typeAlias type="kr.or.ddit.vo.EntRelorStandVO" alias="entRelorStandVO" />
		<typeAlias type="kr.or.ddit.vo.EmpVO" alias="empVO" />
		<!-- 강사 -->
		<typeAlias type="kr.or.ddit.vo.InstrsVO" alias="instrsVO" />
		<typeAlias type="kr.or.ddit.vo.AtchFileDetailVO" alias="atchFileDetailVO" />
		<!-- 강사프로필 -->
		<typeAlias type="kr.or.ddit.vo.InstrsWorkPflVO" alias="instrsWorkPflVO" />
		<!-- 경력사항 -->
		<typeAlias type="kr.or.ddit.vo.CrerQlfcVO" alias="crerQlfcVO" />
		<!-- 보유자격 -->
		<typeAlias type="kr.or.ddit.vo.CotnQuaVO" alias="cotnQuaVO" />
	</typeAliases>
</configuration>


